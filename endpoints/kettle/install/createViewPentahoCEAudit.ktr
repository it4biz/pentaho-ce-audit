    <quoteString>N</quoteString>
    <sql>CREATE VIEW pentaho_ce_audit AS
    SELECT 
	PA.AUDIT_TIME as data, 
	PA.JOB_ID as job, 
	PA.ACTOR as usuario, 
	PA.OBJ_ID as arquivo, 
	PA.OBJ_ID as diretorio, 
	PA.DURATION as duracao,
	PA.MESSAGE_TYPE as tipo,
	PA.MESSAGE_NAME as msg_name,
	PA.MESSAGE_TEXT_VALUE as msg_value
FROM pro_audit PA
where PA.job_id!='Session startup actions' AND PA.job_id!='Global startup actions';</sql>
